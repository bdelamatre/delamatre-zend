{
    "name": "bdelamatre/delamatre-zend",
    "description": "Custom ZF2 extensions created by Byron DeLaMatre",
    "type": "library",
    "keywords": [
        "zf2",
        "delamatre",
        "byron delamatre"
    ],
    "authors": [
        {
            "name": "Byron DeLaMatre",
            "email": "byron@delamatre.com",
            "homepage": "http://delamatre.com"
        }
    ],
    "require": {
        "zendframework/zendframework": "~2.5",
        "doctrine/doctrine-orm-module": "0.*",
        "zf-commons/zfc-base": "0.*",
        "zf-commons/zfc-user": "1.*",
        "zf-commons/zfc-user-doctrine-orm" : "1.*",
        "widmogrod/zf2-assetic-module": "1.*",
        "neilime/zf2-twb-bundle": "2.*@stable",
        "geoip2/geoip2": "2.4.*",
        "google/apiclient" : "^2.0",
        "getresponse/getresponse" : "0.*",
        "khill/lavacharts" : "3.0.*"
    },
    "autoload":{
        "psr-0":{
            "DelamatreZend" : "src/"
        },
        "classmap": [
            "Module.php",
            "src/salesforcetoolkitphp/"
        ]
    }
